<?xml version="1.0" encoding="UTF-8"?>
<migration urlid="http://www.microsoft.com/migration/1.0/migxmlext/customregistry">
    <component type="System">
        <displayName>Registry Migration</displayName>
        <role role="Settings">
            <rules context="UserAndSystem">
                <include>
                    <objectSet>
                        <!-- 迁移特定的注册表项 -->
                        <pattern type="Registry">HKEY_USERS\替换用户sid\SOFTWARE\*
                            HKEY_CURRENT_USER\SOFTWARE\*</pattern>
                    </objectSet>
                </include>
                <exclude>
                    <objectSet>
                        <!-- 排除特定的注册表项 -->
                        <pattern type="Registry">HKEY_USERS\替换用户sid\SOFTWARE\Microsoft
                            HKEY_CURRENT_USER\SOFTWARE\Microsoft</pattern>
                        <pattern type="Registry">HKEY_USERS\替换用户sid\SOFTWARE\Classes
                            HKEY_CURRENT_USER\SOFTWARE\Classes</pattern>
                    </objectSet>
                </exclude>
            </rules>
        </role>
    </component>
</migration>